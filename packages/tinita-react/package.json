{
  "name": "tinita-react",
  "version": "0.0.2",
  "description": "React hooks for the tinita ecosystem - tree-shakeable, type-safe utilities for React applications",
  "author": {
    "name": "DungGramer",
    "email": "dung.dev.gramer@gmail.com",
    "url": "https://dunggramer.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dunggramer/tinita.git",
    "directory": "packages/tinita-react"
  },
  "homepage": "https://github.com/dunggramer/tinita/tree/main/packages/tinita-react#readme",
  "bugs": {
    "url": "https://github.com/dunggramer/tinita/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/styles.css",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./ui": {
      "import": "./dist/ui.mjs",
      "require": "./dist/ui.cjs",
      "types": "./dist/ui.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks.mjs",
      "require": "./dist/hooks.cjs",
      "types": "./dist/hooks.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "sideEffects": [
    "./dist/styles.css"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "generate:exports": "node ../../scripts/generate-package-exports.mjs",
    "clean": "rimraf dist",
    "build:js": "tsup",
    "build:css": "node scripts/build-css.mjs",
    "build": "pnpm run clean && pnpm run build:js && pnpm run build:css",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "check-types": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "pnpm run generate:exports && pnpm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-links": "^10.1.2",
    "@storybook/react": "^10.1.2",
    "@storybook/react-vite": "^10.1.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "cssnano": "^7.1.2",
    "eslint-plugin-storybook": "10.1.2",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^10.1.2",
    "tailwindcss": "^4.1.17",
    "vite": "^7.2.6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/DungGramer"
  },
  "keywords": [
    "react",
    "hooks",
    "typescript",
    "tree-shaking",
    "type-safe",
    "validation",
    "file-utils",
    "uuid",
    "utilities",
    "esm",
    "cjs"
  ],
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "lucide-react": "^0.555.0",
    "motion": "^12.23.25"
  }
}
