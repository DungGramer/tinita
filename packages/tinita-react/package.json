{
  "name": "tinita-react",
  "version": "0.0.2",
  "description": "React hooks for the tinita ecosystem - tree-shakeable, type-safe utilities for React applications",
  "source": "./src/index.ts",
  "author": {
    "name": "DungGramer",
    "email": "dung.dev.gramer@gmail.com",
    "url": "https://dunggramer.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dunggramer/tinita.git",
    "directory": "packages/tinita-react"
  },
  "homepage": "https://github.com/dunggramer/tinita/tree/main/packages/tinita-react#readme",
  "bugs": {
    "url": "https://github.com/dunggramer/tinita/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/styles.css",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./hooks/useToggle": {
      "types": "./dist/hooks/useToggle.d.ts",
      "import": "./dist/hooks/useToggle.mjs",
      "require": "./dist/hooks/useToggle.cjs"
    },
    "./tailwind-preset": {
      "types": "./dist/tailwind-preset.d.ts",
      "import": "./dist/tailwind-preset.mjs",
      "require": "./dist/tailwind-preset.cjs"
    },
    "./styles/globals.css": "./dist/styles/globals.css",
    "./styles.css": "./dist/styles.css",
    "./ui/file-tree": {
      "types": "./dist/ui/file-tree/index.d.ts",
      "import": "./dist/ui/file-tree/index.mjs",
      "require": "./dist/ui/file-tree/index.cjs"
    },
    "./ui/ping": {
      "types": "./dist/ui/ping/index.d.ts",
      "import": "./dist/ui/ping/index.mjs",
      "require": "./dist/ui/ping/index.cjs"
    },
    "./utils/autoInjectStyles": {
      "types": "./dist/utils/autoInjectStyles.d.ts",
      "import": "./dist/utils/autoInjectStyles.mjs",
      "require": "./dist/utils/autoInjectStyles.cjs"
    }
  },
  "sideEffects": [
    "./dist/styles.css",
    "./dist/ui/**/*.css"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "concurrently \"tsup --watch\" \"pnpm run build:css --watch\"",
    "build:js": "tsup",
    "build:css": "node scripts/build-css.mjs",
    "build": "pnpm run clean && pnpm run build:js && pnpm run build:css",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "cssnano": "^7.1.2",
    "concurrently": "^9.1.2",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "vite": "^7.2.6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/DungGramer"
  },
  "keywords": [
    "react",
    "hooks",
    "typescript",
    "tree-shaking",
    "type-safe",
    "validation",
    "file-utils",
    "uuid",
    "utilities",
    "esm",
    "cjs"
  ],
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "lucide-react": "^0.555.0",
    "motion": "^12.23.25"
  }
}
