{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
   "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@tinita-internal/*": ["packages/core/src/*"]
    }
  }
}
